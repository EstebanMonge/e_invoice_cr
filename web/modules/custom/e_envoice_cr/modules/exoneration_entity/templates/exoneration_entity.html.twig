{#
/**
 * @file exoneration_entity.html.twig
 * Default theme implementation to present Exoneration entity data.
 *
 * This template is used when viewing Exoneration entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_exoneration_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('exoneration_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
